/{
	reserved-memory {
		secure_mem: secure_region@0 {
			status = "disabled";
		};
	};
};

&soc {
	qcom,ion {
		qcom,ion-heap@8 { /* CP_MM HEAP  */
			status = "disabled";
		};
	};
};

&apc_vreg_corner {
	/delete-property/ qcom,cpr-init-voltage-adjustment;
	/delete-property/ qcom,cpr-quotient-adjustment;

	qcom,cpr-init-voltage-adjustment =
		<20000 20000 20000>,
		<20000 20000 40000>,
		<20000 20000 40000>,
		<20000 20000 40000>,
		<20000 20000 40000>,
		<20000 20000 45000>;

	qcom,cpr-quotient-adjustment =
		<38    38    38>,
		<76    38    66>, /* SVSP(20mv); TURBO(15mv); KV(1.9) */
		<38    38    66>, /* TURBO(15mv); KV(1.9) */
		<38    38    66>, /* TURBO(15mv); KV(1.9) */
		<76    38    66>, /* SVSP(20mv); TURBO(15mv); KV(1.9) */
		<38    38    76>; /* TURBO(20mv); KV(1.9) */

	qcom,cpr-virtual-corner-init-voltage-adjustment =
		<0    0    0   0   0    25000     0>,
		<0    0    0   0   0    25000     0>,
		<0    0    0   0   0    25000     0>,
		<0    0    0   0   0    25000     0>,
		<0    0    0   0   0    25000     0>,
		<0    0    0   0   0    25000     0>;

	qcom,cpr-virtual-corner-quotient-adjustment =
		<0    0    0    0    0    47     0>,
		<0    0    0    0    0    47     0>,
		<0    0    0    0    0    47     0>,
		<0    0    0    0    0    47     0>,
		<0    0    0    0    0    47     0>,
		<0    0    0    0    0    47     0>;
};
